.title-page{
    color: #000000;
    font-weight: 600;
    font-size: 22px;
    margin: 0;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    line-height: 1.7rem;
    letter-spacing: 1px;
    margin-bottom: 5px;
    margin-top: 0px;
}

.dark-mode .title-page {
    color: #D9D2C3;
}

/* MOBILE */
@media (max-width: 992px) {
    .title-page{
        font-size: 20px;
    }
}