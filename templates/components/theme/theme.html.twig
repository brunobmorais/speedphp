<!DOCTYPE html>
<html lang='pt-BR'>
    <head>
        {{ head | raw}}
        {% for item in css %}
            {{ item | raw }}
        {% endfor %}
    </head>
    <body>
        {{ navbar | raw }}
        {{ title | raw }}
        {% for item in components.top %}
            {% include item %}
        {% endfor %}
        {{ main | raw}}
        {% for item in components.bottom %}
            {% include item %}
        {% endfor %}
        {{ menu | raw }}
        {{ footer | raw }}
    </body>
    {{ javascript | raw}}
    {% for item in js %}
        {{ item | raw }}
    {% endfor %}
</html>